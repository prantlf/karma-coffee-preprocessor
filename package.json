{
  "name": "@prantlf/karma-coffee-preprocessor",
  "version": "1.0.1",
  "description": "A Karma plugin. Compile coffee script on the fly.",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "denolint",
    "check": "grunt",
    "test": "denolint && c8 grunt",
    "version": "grunt npm-contributors"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/prantlf/karma-coffee-preprocessor.git"
  },
  "keywords": [
    "karma-plugin",
    "karma-preprocessor",
    "coffee-script",
    "coffeescript",
    "coffee"
  ],
  "author": "Ferdinand Prantl <prantlf@gmail.com>",
  "c8": {
    "100": true,
    "include": [
      "index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "peerDependencies": {
    "coffeescript": "^2",
    "karma": "^4 || ^5 || ^6"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/karma-coffee-preprocessor/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/karma-coffee-preprocessor#readme",
  "bugs": "https://github.com/prantlf/karma-coffee-preprocessor/issues",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "c8": "^7.13.0",
    "coffeescript": "^2.7.0",
    "denolint": "2.0.7",
    "grunt": "^1.6.1",
    "grunt-karma": "^4.0.2",
    "grunt-npm": "0.0.2",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "load-grunt-tasks": "^5.1.0",
    "mocha": "^10.2.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "failComment": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "contributors": [
    "Ferdinand Prantl <prantlf@gmail.com>",
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
    "Friedel Ziegelmayer <friedel.ziegelmayer@gmail.com>",
    "Niall Smart <niall@pobox.com>",
    "Zachary Wright Heller <zheller@gmail.com>",
    "dignifiedquire <dignifiedquire@gmail.com>",
    "Avindra Goolcharan <aavindraa@gmail.com>",
    "Mark Ethan Trostler <mark@zzo.com>",
    "Anthony Torres <anthony.torres@issinc.com>",
    "Chris Dawson <xrdawson@gmail.com>",
    "Isaac Shapira <fresheyeball@gmail.com>",
    "Johannes WuÌˆrbach <johannes.wuerbach@googlemail.com>",
    "Kyle <kyle.zeeuwen@gmail.com>",
    "Maksim Ryzhikov <rv.maksim@gmail.com>",
    "Michael Kebbekus <michael.kebbekus@goodeggs.com>",
    "Will Scullin <scullin@looker.com>",
    "ehuss <eric@huss.org>"
  ]
}
